 return a + b + c;
  }
  else if (a == b && b != c){
    return c;
  }
  else if (a == c && b != c && b != a){
    return b;
  }
  else if (a == b && b == c && a == c){
    return 0;
  }
  else if (a != b && b == c && a != c){
    return a;
  }
  else if (a == b && b != c && a != c){
    return b + c;
  }
}
loneSum(1, 2, 3); // → 6
loneSum(3, 2, 3); // → 2
loneSum(3, 3, 3); // → 0
