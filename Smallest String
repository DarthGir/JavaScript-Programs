function findMinLengthOfThreeWords(word1, word2, word3){
  // your code here
  var a = word1;
  var b = word2;
  var c = word3;
  var a1 = word1.length;
  var b1 = word2.length;
  var c1 = word3.length;
  return Math.min(a1, b1, c1);
}
var output = findMinLengthOfThreeWords('a', 'be', 'see');
console.log(output); // --> 1
